<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
		<referenceBlock name="sales.order.view">
			<action method="setTemplate">
				<argument name="template" xsi:type="string">Fcamodule_Addsample::reorder/sidebar.phtml</argument>
			</action>
		</referenceBlock>
		<referenceBlock name="sales.order.items.renderers">
			<action method="setTemplate">
				<argument name="template" xsi:type="string">Fcamodule_Addsample::reorder/default.phtml</argument>
			</action>
		</referenceBlock>
    </body>
</page>
